{
  "mount": {
    "example/target/scala-2.13/example-fastopt-test-html": "/",
    "public": "/",
    "example/target/scala-2.13": "/_dist_",
    "src": "/_dist_"
  },
  "plugins": [
    [
      "@snowpack/plugin-run-script",
      {
        "cmd": "tsc --noEmit",
        "watch": "$1 --watch"
      }
    ],
    [
      "@snowpack/plugin-run-script",
      {
        "cmd": "sbtn example/fastOptJS",
        "watch": "sbtn ~example/fastOptJS"
      }
    ]
  ],
  "installOptions": {
    "installTypes": true
  }
}
